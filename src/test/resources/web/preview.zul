<zk xmlns:h="native">
    <script src="http://localhost:50000/socket.io/socket.io.js"/>
    <script src="http://localhost:50000/zklessLiveReloadStyles.js"/>
    <!--	<script src="http://localhost:50000/zklessLiveReloadStylesResize.js"/>-->
    <!--	<script src="http://localhost:50000/zklessLiveReloadPage.js"/>-->

    <zscript>
        Date now = new Date();
    </zscript>
    <window title="Window" border="normal" closable="true" maximizable="true" minimizable="true">
        <grid>
            <columns>
                <column width="200px"/>
                <column/>
            </columns>
            <rows>
                <row>
                    Buttons
                    <div>
                        <button label="Button"/>
                        <button iconSclass="z-icon-save"/>
                        <button label="Icon" iconSclass="z-icon-times"/>
                        <button label="disabled" disabled="true"/>
                    </div>
                </row>
                <row>
                    Checkboxes
                    <div>
                        <checkbox label="checkbox"/>
                        <checkbox label="checkbox" checked="true"/>
                        <separator/>
                        <checkbox label="toggle" mold="toggle"/>
                        <checkbox label="toggle" mold="toggle" checked="true"/>
                        <separator/>
                        <checkbox label="switch" mold="switch"/>
                        <checkbox label="switch" mold="switch" checked="true"/>
                    </div>
                </row>
                <row>
                    Inputs
                    <div>
                        <textbox value="textbox"/>
                        <textbox value="readonly" readonly="true"/>
                        <textbox value="disabled" disabled="true"/>
                        <separator/>
                        <datebox value="${now}"/>
                        <datebox value="${now}" readonly="true"/>
                        <datebox value="${now}" disabled="true"/>
                        <separator/>
                        <spinner value="100"/>
                        <spinner value="200" readonly="true"/>
                        <spinner value="300" disabled="true"/>
                        <separator/>
                    </div>
                </row>
                <row>
                    Progressmeter
                    <progressmeter value="40" hflex="1"/>
                </row>
                <row>
                    Listbox
                    <listbox checkmark="true" multiple="true">
                        <listhead>
                            <listheader label="col 1"/>
                            <listheader label="col 2"/>
                            <listheader label="col 3"/>
                        </listhead>
                        <listitem>
                            <listcell label="aaa"/><listcell label="bbb"/><listcell label="ccc"/>
                        </listitem>
                        <listitem selected="true">
                            <listcell label="aaa"/><listcell label="bbb"/><listcell label="ccc"/>
                        </listitem>
                        <listitem>
                            <listcell label="aaa"/><listcell label="bbb"/><listcell label="ccc"/>
                        </listitem>
                    </listbox>
                </row>
            </rows>
        </grid>
    </window>
    <h:h1>horizontal</h:h1>
    <menubar width="400px" scrollable="true">
        <menuitem label="item 1" iconSclass="z-icon-circle z-icon-regular"/>
        <menuitem label="item 2" checkmark="true"/>
        <menuitem label="item 3" iconSclass="z-icon-circle z-icon-regular"/>
        <menuitem label="item 4" iconSclass="z-icon-circle z-icon-regular"/>
        <menuitem label="item 5" iconSclass="z-icon-circle z-icon-regular"/>
    </menubar>
    <h:h1>vertical</h:h1>
    <menubar orient="vertical" scrollable="true">
        <menuitem label="item 1" iconSclass="z-icon-circle z-icon-regular"/>
        <menuitem label="item 2" />
        <menuitem label="item 3"/>
        <menuitem label="item 4"/>
        <menuitem label="item 5"/>
        <menuseparator/>
        <menuitem label="item 6" checkmark="true"/>
    </menubar>
    <h:h1>menupopup</h:h1>
    <menubar>
        <menu label="File" iconSclass="z-icon-file">
            <menupopup>
                <menuitem label="New" iconSclass="z-icon-file"/>
                <menu label="Open" iconSclass="z-icon-folder-open">
                    <menupopup>
                        <menuitem label="Project..." iconSclass="z-icon-folder"/>
                        <menuitem label="File..." iconSclass="z-icon-file"/>
                    </menupopup>
                </menu>
                <menuitem label="Save" iconSclass="z-icon-save"/>
                <menuitem label="Exit" iconSclass="z-icon-times"/>
            </menupopup>
        </menu>
        <menu label="View">
            <menupopup>
                <menuitem label="Sort by Name" checkmark="true" autocheck="true" />
                <menuitem label="Sort by Date" checkmark="true" autocheck="true" />
            </menupopup>
        </menu>
        <menuseparator/>
        <menuitem label="Help" iconSclass="z-icon-question"/>
    </menubar>
    <h:h1>Toolbar</h:h1>
    <toolbar>
        <toolbarbutton label="New" iconSclass="z-icon-file"/>
        <toolbarbutton label="Open" iconSclass="z-icon-folder-open"/>
        <toolbarbutton label="Save" iconSclass="z-icon-save"/>
    </toolbar>
    <h:h1>Toolbar in tabbox</h:h1>
    <tabbox>
        <toolbar>
            <toolbarbutton label="New" iconSclass="z-icon-file"/>
            <toolbarbutton label="Open" iconSclass="z-icon-folder-open"/>
            <toolbarbutton label="Save" iconSclass="z-icon-save"/>
        </toolbar>
        <tabs>
            <tab label="Tab 1"/>
            <tab label="Tab 2"/>
            <tab label="Tab 3"/>
        </tabs>
        <tabpanels>
            <tabpanel/>
            <tabpanel/>
            <tabpanel/>
        </tabpanels>
    </tabbox>

    <h:h1>Navbar</h:h1>
    <navbar>
        <navitem label="Home" iconSclass="z-icon-home"/>
        <navitem label="Dashboard" iconSclass="z-icon-tachometer"/>
        <navitem label="Products" iconSclass="z-icon-cube"/>
        <navitem label="Orders" iconSclass="z-icon-shopping-cart"/>
        <navitem label="Reports" iconSclass="z-icon-bar-chart"/>
        <navitem label="Settings" iconSclass="z-icon-cog"/>
    </navbar>

    <h:h1>Navbar - vertical</h:h1>
    <navbar orient="vertical">
        <navitem label="Home" iconSclass="z-icon-home"/>
        <nav label="Products" iconSclass="z-icon-cube">
            <navitem label="All Products" iconSclass="z-icon-list"/>
            <navitem label="Categories" iconSclass="z-icon-tags"/>
            <navitem label="Inventory" iconSclass="z-icon-warehouse"/>
        </nav>
        <nav label="Sales" iconSclass="z-icon-shopping-cart">
            <navitem label="Orders" iconSclass="z-icon-file-text"/>
            <navitem label="Customers" iconSclass="z-icon-users"/>
            <navitem label="Analytics" iconSclass="z-icon-pie-chart"/>
        </nav>
        <navitem label="Support" iconSclass="z-icon-life-ring"/>
        <navseparator/>
        <navitem label="Profile" iconSclass="z-icon-user"/>
    </navbar>
</zk>