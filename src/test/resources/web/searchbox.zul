<?page title="Searchbox" contentType="text/html;charset=UTF-8"?>
<zk>
	<zscript>
	ListModel model4Searchbox = new ListModelArray(new String[] {
	  "North America", "South America", "Europe", "Asia", "Africa", "Oceania", "Antarctica"
	});
	</zscript>
	<searchbox model="${model4Searchbox}" placeholder="An unknown place" autoclose="true">
	   <template name="model">
	       <html><![CDATA[
	       <i class="z-icon-globe"></i> ${each}
	       ]]></html>
	   </template>
	</searchbox>
</zk>
